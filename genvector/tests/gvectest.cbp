			<Option target="Test Modular" />
		</Unit>
		<Unit filename="../../internals/munit/munit.h">
			<Option virtualFolder="munit/" />
			<Option target="Test Static" />
			<Option target="Test Modular" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
